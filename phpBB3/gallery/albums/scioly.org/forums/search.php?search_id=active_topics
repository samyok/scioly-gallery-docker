	var banner_id = "banner_" + 15;
	var banner_boolean = localStorage.getItem(banner_id);

	$(document).ready(function() {
		if (banner_boolean == "false") {
			$('.banner').hide();
		}
	});

	function close_banner() {
		localStorage.setItem(banner_id, "false");
	}
</script>
